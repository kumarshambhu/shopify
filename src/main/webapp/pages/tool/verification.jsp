<%@page contentType="text/html" pageEncoding="UTF-8"%><%@taglib uri="http://java.sun.com/jsp/jstl/core" prefix="c"%><!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN"    "http://www.w3.org/TR/html4/loose.dtd"><html>    <head>        <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">        <title>Shopify - Online Store</title>        <%@include file="/pages/tool/common/include.jspf"%>    </head>    <body class="ui-widget">        <form action="${BASE_URL}/login" method="POST">            <table>                <tr>                    <td>                        <!-- Header -->                        <%@include file="/pages/tool/common/header.jspf" %>                    </td>                </tr>                <tr>                    <td>                        <!-- Main                    -->                        <table class="page-content-setting">                            <tr>                                <td valign="top" align="center">                                    <table valign="top" align="center">                                        <tr>                                            <td align="center" colspan="2">                                                <br/>                                                <br/>                                            </td>                                        </tr>                                        <tr>                                            <th align="center" colspan="2" class="ui-widget-header">                                                Token Verification                                            </th>                                        </tr>                                        <tr>                                            <td align="center" colspan="2">                                                <br/>                                                <br/>                                            </td>                                        </tr>                                        <tr>                                            <th align="right" width="45%">                                                Verification Code:                                            </th>                                            <td align="left">                                                <input type="hidden" name="profile_id" value="${profile_id}"/>                                                <input type="text" name="verification_code" value="" size="45"/>                                            </td>                                        </tr>                                                                                <tr>                                            <td align="center" colspan="2">                                                <br/>                                                <br/>                                            </td>                                        </tr>                                        <tr>                                            <th colspan="2">                                                <input type="submit" name="action" id="verify" value="Verify" class="ui-button ui-widget ui-state-default ui-corner-all" title="Verify Verification Code"/>                                                <br/>                                            </th>                                        </tr>                                        <tr>                                            <td align="center" colspan="2">                                                <br/>                                                <label style="color:red">A mail has been sent to your account with the verification code!</label>                                                <br/>                                            </td>                                        </tr>                                                                                <tr>                                            <td align="center" colspan="2">                                                <br/>                                                <br/>                                            </td>                                        </tr>                                    </table>                                </td>                            </tr>                        </table>                    </td>                </tr>                <tr>                    <td>                        <!-- Footer -->                        <%@include file="/pages/tool/common/footer.jspf" %>                    </td>                </tr>            </table>        </form>    </body></html>