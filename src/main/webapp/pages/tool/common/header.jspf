<%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core" %><%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core" %><table class="page-header-setting">    <tr>        <td width="5%" style="border-right: 1px solid lightgray;">            <a href="${BASE_URL}/home">                <img src="${BASE_URL}/pages/resources/images/shopify.png" width="200" height="60"/>            </a>        </td>        <td width="100%">            <table width="100%" style="border-style: none">                <tr>                    <td align="right" valign="middle">                        <c:choose>                            <c:when test="${sessionScope.uid != null}">                                Welcome                                   &nbsp;                                <a href="${BASE_URL}/home?action=EditUser&profileId=${sessionScope.uid}">${sessionScope.user}</a>                                &nbsp;&nbsp; | &nbsp;&nbsp;                                <a href="${BASE_URL}/logout">Logout</a>                            </c:when>                            <c:otherwise>                                <a href="${BASE_URL}/login">Login</a>                                &nbsp;&nbsp; | &nbsp;&nbsp;                                <a href="#">Signup</a>                            </c:otherwise>                        </c:choose>                    </td>                </tr>                <tr class="ui-state-highlight">                    <td>                        <ul class="dropdown" >                            <li><a href="${BASE_URL}/home" title="Home">Home</a></li>                                <c:if test="${sessionScope.urole == 'ADMIN'}">                                    <li><a href="${BASE_URL}/product" title="Add Product">Add Product</a></li>                                </c:if>                            <li><a href="${BASE_URL}/order" title="Order">Order</a></li>                            <li><a href="${BASE_URL}/pages/tool/aboutus.jsp" title="About Us">About Us</a></li>                        </ul>                    </td>                </tr>            </table>        </td>    </tr></table>